         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xml:base="http://www.bartender-project.org/ontology"
         xmlns="http://www.bartender-project.org/ontology#">

<owl:Ontology rdf:about="http://www.bartender-project.org/ontology"/>

<owl:ObjectProperty rdf:about="#hasIngredient">
  <rdfs:domain rdf:resource="#Cocktail"/>
  <rdfs:range rdf:resource="#Ingredient"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#servedIn">
  <rdfs:domain rdf:resource="#Cocktail"/>
  <rdfs:range rdf:resource="#Glass"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasPreparation">
  <rdfs:domain rdf:resource="#Cocktail"/>
  <rdfs:range rdf:resource="#Preparation"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#originCountry">
  <rdfs:domain rdf:resource="#Cocktail"/>
